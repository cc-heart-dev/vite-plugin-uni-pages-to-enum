{
  "name": "@cc-heart/vite-plugin-uni-pages-to-enum",
  "version": "0.0.3",
  "type": "module",
  "author": "Carl Chen",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "a plugin that transforms uni-app pages.json",
  "keywords": ["vite-plugin", "uni-app", "pages-to-enum"],
  "files": [
    "dist",
    "package.json"
