      {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    ],
    "./*": "./*"
  },
  "scripts": {
    "compile": "rollup -c"
  },
  "devDependencies": {
    "@cc-heart/utils": "^5.2.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.12.12",
    "json5": "^2.2.3",
    "rollup": "^4.17.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.4.5"
  }
}
